{
    "author": {
        "name": "quantinium",
        "url": "https://quantinium.dev",
        "image": "https://github.com/quantinium03.png",
        "github": "https://github.com/quantinium03",
        "twitter": "https://twitter.com/quantinium_dev"
    },
    "lesson": "C and Rust",
    "months": [
        {
            "month": 1,
            "weeks": [
                {
                    "week": 1,
                    "description": "Learning C",
                    "tasks": [
                        {
                            "type": "Prerequisite",
                            "list": [
                                "Install linux",
                                "If not possible, install and setup WSL"
                            ]
                        },
                        {
                            "type": "Resource",
                            "content": "Beej's Guide to C",
                            "list": [
                                "Complete till 7.1"
                            ],
                            "url": "https://beej.us/guide/bgc/html/split"
                        },
                        {
                            "type": "Video",
                            "content": "Complete C Tutorial If You are Lazy",
                            "url": "https://www.youtube.com/watch?v=87SH2Cn0s9A"
                        },
                        {
                            "type": "Video",
                            "content": "A Must Watch Series On Pointers",
                            "url": "https://www.youtube.com/watch?v=zuegQmMdy8M"
                        },
                        {
                            "type": "Project",
                            "content": "Simple Projects",
                            "list": [
                                "A Simple CLI Calculator",
                                "Try implementing sorting algorithms like Merge Sort, Quick Sort and Bubble Sort"
                            ]
                        }
                    ]
                },
                {
                    "week": 2,
                    "description": "Going deeper into C",
                    "tasks": [
                        {
                            "type": "Resource",
                            "content": "Beej's Guide to C",
                            "list": [
                                "Complete till 15. List of topics: ",
                                "Structs, Enums ",
                                "file I/O",
                                "memory allocation",
                                "Pointer arithmetic",
                                "Type conversions",
                                "Ascii characters",
                                "Header file (managing bigger projects)"
                            ],
                            "url": "https://beej.us/guide/bgc/html/split"
                        },
                        {
                            "type": "Project",
                            "content": "Book Bot",
                            "list": [
                                "Take https://www.gutenberg.org/files/1342/1342-0.txt as input file",
                                "Count how many times each letter and word appears in the book"
                            ]
                        },
                        {
                            "type": "Project",
                            "content": "Arrays",
                            "list": [
                                "Implement data structures like stacks, queues, linked lists and doubly linked lists"
                            ]
                        }
                    ]
                },
                {
                    "week": 3,
                    "description": "Even Deeper into C",
                    "tasks": [
                        {
                            "type": "Video",
                            "content": "Processes in C",
                            "url": "https://www.youtube.com/watch?v=cex9XrZCU14&list=PLfqABt5AS4FkW5mOn2Tn9ZZLLDwA3kZUY"
                        },
                        {
                            "type": "Video",
                            "content": "Signals in C",
                            "url": "https://youtu.be/83M5-NPDeWs?feature=shared"
                        },
                        {
                            "type": "Video",
                            "content": "Syscalls",
                            "url": "https://www.youtube.com/watch?v=fLS99zJDHOc"
                        },
                        {
                            "type": "Video",
                            "content": "Tokenisation",
                            "url": "https://www.youtube.com/watch?v=nrO_pXGZc3Y"
                        },
                        {
                            "type": "Project",
                            "content": "Make a mini grep",
                            "list": [
                                "Try using grep yourself and implement different things"
                            ],
                            "urls": [
                                "https://www.geeksforgeeks.org/grep-command-in-unixlinux/",
                                "https://www.gnu.org/software/grep/manual/grep.html"
                            ]
                        },
                        {
                            "type": "Project",
                            "content": "Implement your own hash table",
                            "list": [
                                "Learn about hash tables, collision management, hashing algorithms"
                            ],
                            "url": "https://github.com/jamesroutley/write-a-hash-table"
                        },
                        {
                            "type": "Optional Project",
                            "content": "Implement your own toy shell",
                            "url": "https://brennan.io/2015/01/16/write-a-shell-in-c/",
                            "list": [
                                "Learn about processes, signals, syscalls, tokenisation"
                            ]
                        }
                    ]
                },
                {
                    "week": 4,
                    "description": "Final Project with C",
                    "tasks": [
                        {
                            "type": "Project",
                            "content": "Make a web server with C",
                            "urls": [
                                "https://github.com/namishh/webby",
                                "https://beej.us/guide/bgnet/html/split/",
                                "https://dev.to/jeffreythecoder/how-i-built-a-simple-http-server-from-scratch-using-c-739"
                            ],
                            "details": {
                                "features": [
                                    "Sending JSON, HTML",
                                    "Serving static files like images, css, js",
                                    "Simple routing system",
                                    "Handling GET and POST requests",
                                    "Handling errors like 404 and 500",
                                    "Try to make a full stack todo app",
                                    "OPTIONAL: Try implementing some kind of middleware"
                                ]
                            }
                        },
                        {
                            "type": "Video",
                            "content": "Threads in C",
                            "url": "https://www.youtube.com/watch?v=d9s_d28yJq0&list=PLfqABt5AS4FmuQf70psXrsMLEDQXNkLq2"
                        },
                        {
                            "type": "Video",
                            "content": "A Good Starting Point",
                            "url": "https://www.youtube.com/watch?v=gk6NL1pZi1M"
                        }
                    ]
                }
            ]
        },
        {
            "month": 2,
            "weeks": [
                {
                    "week": 1,
                    "description": "Starting with Rust",
                    "tasks": [
                        {
                            "type": "Resource",
                            "content": "Rust Book",
                            "url": "https://doc.rust-lang.org/book/",
                            "list": ["Read till Chapter 5"]
                        },
                        {
                            "type": "Video",
                            "content": "Let's Get Rusty",
                            "list": ["Literally the video version of the rust book."],
                            "url": "https://www.youtube.com/watch?v=OX9HJsJUDxA&list=PLai5B987bZ9CoVR-QEIN9foz4QCJ0H2Y8"
                        },
                        {
                            "type": "Project",
                            "content": "Simple Number Guessing Game",
                            "url": "https://rust-book.cs.brown.edu/ch02-00-guessing-game-tutorial.html"
                        }, 
                        {
                            "type": "Exercise",
                            "content": "Solve rustlings",
                            "url": "https://github.com/rust-lang/rustlings"
                        },
                        {
                            "type": "Project",
                            "content": "Calculate area of different shapes",
                            "list": ["Try to use structs and impls"]
                        } 
                    ]
                },
                {
                    "week": 2,
                    "description": "Memory and Rust",
                    "tasks": [
                        {
                            "type": "Resource",
                            "content": "Playlist on Memory Management",
                            "url": "https://www.youtube.com/playlist?list=PLai5B987bZ9BnS7dBlTF9HRop55vYUL7r"
                        },
                        {
                            "type": "Resource",
                            "content": "Rust Book",
                            "url": "https://doc.rust-lang.org/book/",
                            "list": ["Read till Chapter 13"]
                        },
                        {
                            "type": "Project",
                            "content": "Cloning simple GNU tools: cat and wc",
                            "urls": [
                                "https://man7.org/linux/man-pages/man1/cat.1.html",
                                "https://linux.die.net/man/1/wc"
                            ]
                        }
                    ]
                },
                {
                    "week": 3,
                    "description": "2 Small Projects",
                    "tasks": [
                        {
                            "type": "Project",
                            "content": "Clone grep",
                            "urls": [
                                "https://www.geeksforgeeks.org/grep-command-in-unixlinux/",
                                "https://www.gnu.org/software/grep/manual/grep.html"
                            ]
                        },
                        {
                            "type": "Project",
                            "content": "Implement stack from",
                            "url": "https://rust-unofficial.github.io/too-many-lists/index.html"
                        }
                    ]
                },
                {
                    "week": 4,
                    "description": "Smart Pointers, Concurrency and Async",
                    "tasks": [
                        {
                            "type": "Resource",
                            "content": "Rust Book",
                            "url": "https://doc.rust-lang.org/book/",
                            "list": ["Read till Chapter 18"]
                        },
                        {
                            "type": "Project",
                            "content": "Implement dequeue from",
                            "url": "https://rust-unofficial.github.io/too-many-lists/index.html"
                        },
                        {
                            "type": "Project",
                            "content": "Desktop Tracker",
                            "list": [
                                "Make a tracker for yourself, counts number of keypresses, mouse click, mouse travel, screenshot images every defined interval and saves them."
                            ],
                            "details": {
                                "examples" : [
                                    "https://github.com/nanovin/gaze"
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}